<% include partials/header %>

<h1>Welcome to posts page</h1>
<ul>
<% posts.forEach(function(post) { %>

    <li class="posts-single">
        <h3><%= post.words %></h3>
        <small><%= post.sentence %></small>
        <p><%= post.sentences %></p>
        <p><%= post.paragraph %></p>
    </li>
<% }); %>
</ul>

<% include partials/footer %>